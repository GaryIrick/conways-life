{
  "name": "conways-life",
  "version": "1.0.0",
  "description": "Conway's game of life in React",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "dev": "nodemon --inspect --watch . --ignore test --ignore src -e js,md,html server.js",
    "test": "jest --ci --coverage",
    "test:dev": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GaryIrick/conways-life.git"
  },
  "jest": {
    "projects": [
      {
        "testMatch": [
          "<rootDir>/test/**/*Spec.js"
        ],
        "setupFiles": [
          "./test/setup.js"
        ],
        "verbose": false,
        "snapshotSerializers": [
          "enzyme-to-json/serializer"
        ]
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "lint",
        "testMatch": [
          "<rootDir>/src/**/*.js",
          "<rootDir>/test/**/*Spec.js"
        ]
      }
    ]
  },
  "author": "Gary Irick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GaryIrick/conways-life/issues"
  },
  "homepage": "https://github.com/GaryIrick/conways-life#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.2.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "jest": "^23.6.0",
    "jest-runner-eslint": "^0.7.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "superagent": "^4.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
